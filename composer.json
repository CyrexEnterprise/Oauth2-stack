{
	"name": "cloudoki/oauth2-stack",
	"description": "A complete Oauth2 Server and Views stack",
	"homepage": "http://github.com/Cloudoki/oauth2-stack",
	"keywords": ["oauth2","laravel"],
    "license":"MIT",
	"authors": [
		{
			"name": "Koen Betsens",
			"email": "koen@cloudoki.com"
		},
		{
			"name": "Rui Molefas",
			"email": "rui@cloudoki.com"
		}
	],
	"require": {
		"php": ">=7.1.3",
		"laravelcollective/html": "5.6.*",
		"bshaffer/oauth2-server-php": "v1.10.0"
	},
	"autoload": {
		"classmap": [
			"src/Cloudoki/OaStack/Controllers",
			"src/Cloudoki/OaStack/Models",
			"src/Cloudoki/OaStack/Views",
			"src/seeds",
			"src/lang",
			"src/migrations"
		],
		"psr-0": {
			"Cloudoki\\OaStack\\": "src/"
		}
	},
	"minimum-stability": "stable"
}
